{
  // Strengthen Restrictions.
  "eqeqeq": true,
  "immed": true,
  "freeze": true,
  "latedef": "nofunc",
  "newcap": true,
  "noarg": true,
  "nonew": true,
  "quotmark": "double",
  "strict": true,
  "undef": true,
  "unused": "vars",
  "trailing": true,

  // Predefinitions.
  "predef": [
    "define",
    "Firebug"
  ]
}
